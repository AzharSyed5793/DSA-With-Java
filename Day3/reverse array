import java.util.*;

public class JavaBasics {

  public static void reverse(int num[]){
    int first=0, last=num.length-1;
    while(first<last){
      //swap
      int temp = num[last];
      num[last] = num[first];
      num[first] = temp;
      first++;
      last--;
    }
  }
  
 public static void main(String args[]){
 int num[] = {10,50,70,57,82,44};
 reverse(num);

 for(int i=0; i<num.length; i++){
  System.out.println(num[i]+" ");
 }
 System.out.println();
  }
    
}
