import java.util.*;

public class arrays {

  public static void printPairs(int num[]){
    for(int i=0; i<num.length; i++){
      int curr = num[i];
      for(int j=i+1; j<num.length; j++){
        System.out.println("("+curr+","+num[j]+")");
      }
      System.out.println();
    }
  }
  
 public static void main(String args[]){
 int num[] = {10,50,70,57,82,44};
 printPairs(num);
  }  
}

